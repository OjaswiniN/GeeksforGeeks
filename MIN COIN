         
 // { Driver Code Starts
#include<bits/stdc++.h>
using namespace std;

 // } Driver Code Ends
class Solution{
	public:
	int MinCoin(vector<int>a, int x)
	{
	int n=a.size();
    int dp[n+1][x+1];
    for(int i=0;i<n+1;i++)
    {
        dp[i][0]=0;
    }
    for(int j=0;j<x+1;j++)
    {
        dp[0][j]=INT_MAX-1;
    }
    for(int i=1;i<n+1;i++)
    {
        for(int j=1;j<x+1;j++)
        {
            if(a[i-1]<=j)
            {
                dp[i][j]=min(dp[i][j-a[i-1]]+1,dp[i-1][j]);
            }
            else
            {
                dp[i][j]=dp[i-1][j];
            }
        }
    }
    return dp[n][x]==INT_MAX-1?-1:dp[n][x];

	}
};

// { Driver Code Starts.
int main(){
	int tc;
	cin >> tc;
	while(tc--){
		int n, amount;
		cin >> n >> amount;
		vector<int>nums(n);
		for(int i = 0; i < n; i++)
			cin >> nums[i];
		Solution ob;
		int ans = ob.MinCoin(nums, amount);
		cout << ans <<"\n";
	}
	return 0;
}  // } Driver Code Ends
