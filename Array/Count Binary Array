int countOnes(int arr[], int N)
    {
       int low =0,high = N-1;
       int count =0;
       while(low<=high){
           int mid= (low+high)/2;
           if(arr[mid]==1){
               count = mid+1;
               low=mid+1;
           }
           else if(arr[mid]==0){
               high = mid-1;
           }
       }
       return count;
   
        
    }
