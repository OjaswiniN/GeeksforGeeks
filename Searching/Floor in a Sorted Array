int low = 0;
       int high = n-1;
       int ans = -1;
       
       while(low<=high)
       {
           int mid = low + (high-low)/2;
               
           if(arr[mid] == x)
               return mid;
           else if(arr[mid] > x)
               high = mid-1;
           else
           {
               ans = mid;
               low = mid+1;
           }
               
       }
       return ans;
   }

